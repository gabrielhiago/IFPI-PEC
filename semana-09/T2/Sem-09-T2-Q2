def cancao():
    for i in range(99, 251):
        if i < 250:
            print(f'{i} bugs no software, pegue um deles e conserte...\nTecle "Ctrl+F5"')
        else:
            print(f'{i} bugs no software, pegue um deles e conserte...\nTecle "Ctrl+F5"')
            print(f'Vamos fazer mais um cafÃ©!')

def main():
    cancao()

if __name__ == '__main__':
    main()
